<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.canvas.Canvas?>
<AnchorPane fx:controller="Uniplay.Workbench.NG2DLevelDesignerStageController" xmlns:fx="http://javafx.com/fxml" >
    <AnchorPane prefHeight="50" minWidth="800" xmlns:fx="http://javafx.com/fxml" AnchorPane.TopAnchor="0">
        <HBox prefHeight="50" minWidth="800" alignment="TOP_LEFT" spacing="10" xmlns:fx="http://javafx.com/fxml" style="-fx-background-color: gray;">
            <padding><Insets top="15" right="15" bottom="15" left="15"/></padding>
            <Button id="btnLoadGOF" prefWidth="150" text="Load GOF File" onAction="#handleLoadGOF" fx:id="btnLadGOF" />
        </HBox>
    </AnchorPane>
    <AnchorPane prefHeight="750" minWidth="750" xmlns:fx="http://javafx.com/fxml" AnchorPane.bottomAnchor="0">
        <Canvas id="Layer1" height="750" width="800" AnchorPane.leftAnchor="0" AnchorPane.topAnchor="0" fx:id="Layer1" />
        <Canvas id="Layer0" height="750" width="800" AnchorPane.leftAnchor="0" AnchorPane.topAnchor="0" fx:id="Layer0" />
    </AnchorPane>
</AnchorPane>